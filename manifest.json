{
    "name": "The New App Launcher",
    "description": "Launches apps, the good way!",
    "version": "0.0.0.1",
    "content_scripts": [{
		"js": ["jquery-2.2.0.min.js", "jquery-ui.min.js", "newtab.js"],
		"matches": ["<all_urls>"]
	}],
	"permissions" : ["storage", "management", "activeTab"],
    "chrome_url_overrides" : {
        "newtab": "newtab.html"
    },
    "browser_action": {
        "default_icon":"images/heart-icon.png",
        /* default_icon": {
            "19": "images/chrome.png",
            "38": "images/chrome.png" 
        },*/
        "default_title": "Add a new bookmark",
        "default_popup": "popup.html" 
    },
	"manifest_version": 2
}
