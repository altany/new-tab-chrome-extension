{
    "name": "The New App Launcher",
    "description": "Launches apps, the good way!",
    "version": "0.0.0.1",
    "content_scripts": [{
		"js": ["jquery-2.2.0.min.js", "newtab.js"],
		"matches": ["<all_urls>"]
	}],
	"permissions" : ["storage", "management"],
    "chrome_url_overrides" : {
        "newtab": "newtab.html"
    },
	"manifest_version": 2
}
