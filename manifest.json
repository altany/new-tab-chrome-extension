{
    "name": "New Tab Widget",
    "description": "Customise the new tab",
    "version": "0.0.0.2",
    "content_scripts": [{
		"js": [
            "jquery-2.2.0.min.js"
            , "jquery-ui.min.js"
            , "newtab.js","popup.js"
        ],
		"matches": ["<all_urls>"]
	},
    {
		"js": [
            "jquery-2.2.0.min.js"
            //, "jquery-ui.min.js"
            ,"popup.js"
        ],
		"matches": ["<all_urls>"],
        "run_at": "document_end"
	}],
	"permissions" : [
        "storage"
        , "management"
        , "tabs"
        , "activeTab"
       // , "<all_urls>"
        , "contextMenus"
    ],
    "chrome_url_overrides" : {
        "newtab": "newtab.html"
    },
    "browser_action": {
        "default_icon":"images/chrome.png",
        /*"default_icon": {
            "19": "images/chrome.png",
            "38": "images/chrome.png" 
        },*/
        "default_title": "Add a new bookmark",
        "default_popup": "popup.html" 
    },
	"manifest_version": 2
}
